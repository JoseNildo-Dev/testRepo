<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:background="#ffffff" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/RelativeLayout_back" android:layout_width="60dp" android:layout_height="32dp">
        <ImageView android:layout_width="24dp" android:layout_height="24dp" android:layout_marginLeft="16dp" android:src="@drawable/img_back" android:layout_alignParentBottom="true" android:contentDescription="@null"/>
    </RelativeLayout>
    <TextView android:textSize="23sp" android:textColor="#212121" android:gravity="center" android:id="@+id/tv_title" android:layout_width="match_parent" android:layout_height="90dp" android:text="@string/wifi_help_title" android:layout_below="@+id/RelativeLayout_back"/>
    <TextView android:textSize="12sp" android:textColor="#666666" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="22dp" android:layout_marginRight="22dp" android:text="@string/wifi_help_info" android:layout_below="@+id/tv_title" android:lineSpacingExtra="10dp"/>
</RelativeLayout>
