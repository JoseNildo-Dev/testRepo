<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:background="@drawable/dialog_bg" android:paddingTop="32dp" android:layout_width="310dp" android:layout_height="wrap_content">
    <TextView android:textSize="18sp" android:textColor="#666666" android:gravity="center_horizontal" android:id="@+id/head" android:paddingBottom="16dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="32dp" android:layout_marginRight="32dp" android:text="商米" android:layout_centerHorizontal="true"/>
    <TextView android:textSize="14sp" android:textColor="#222222" android:gravity="center_horizontal" android:id="@+id/msg" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="32dp" android:layout_marginRight="32dp" android:text="的数据库发货速度快时间的话福克斯的金凤凰会计师" android:layout_below="@+id/head" android:layout_centerHorizontal="true"/>
    <View android:id="@+id/line" android:background="@color/CD2D2D2" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_marginTop="32dp" android:layout_below="@+id/msg"/>
    <TextView android:textSize="14sp" android:textColor="#222222" android:gravity="center" android:id="@+id/btnTxt" android:background="@drawable/ripple_bg_bottom" android:clickable="true" android:layout_width="match_parent" android:layout_height="45dp" android:text="按鈕" android:layout_below="@+id/line"/>
</RelativeLayout>
